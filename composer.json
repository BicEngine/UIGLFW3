{
    "name": "bic-engine/ui-glfw3",
    "description": "GLFW3 Driver for UI Component",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ext-ffi": "*",
        "ffi/env": "^1.0",
        "bic-engine/ui": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\UI\\GLFW3\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.28",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\UI\\GLFW3\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
